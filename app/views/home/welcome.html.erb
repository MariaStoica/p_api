<h1>The DataBase</h1>
<br/><br/>


<!-- users -->
Users
<br/>
<table class="table table-striped table hover">
	<thead>
		<tr>
			<th>Id</th>
			<th>FirstName</th>
			<th>LastName</th>
			<th>PhoneNumber</th>
			<th>Auth token</th>
		</tr>
	</thead>
	<tbody>
		<% User.all.each do |user| %>
			<tr>
				<td><%= user.id %></td>
				<td><%= user.first_name %></td>
				<td><%= user.last_name %></td>
				<td><%= user.phone_number %></td>
				<% apikey = ApiKey.find_by_user_id(user.id) %>
				<% if apikey %>
					<td><%= apikey.access_token %></td>
				<% else %>
					<td> - </td>
				<% end %>
			</tr>
		<% end %>
	</tbody>
</table>

<br/><br/>
