<h1>Listing goingtoactivities</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Activity</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @goingtoactivities.each do |goingtoactivity| %>
      <tr>
        <td><%= goingtoactivity.user_id %></td>
        <td><%= goingtoactivity.activity_id %></td>
        <td><%= link_to 'Show', goingtoactivity %></td>
        <td><%= link_to 'Edit', edit_goingtoactivity_path(goingtoactivity) %></td>
        <td><%= link_to 'Destroy', goingtoactivity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

